{
  "name": "upload-service",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bun --watch index.ts",
    "start": "bun index.ts",
    "build": "bun build index.ts --outfile dist/server.js --target bun",
    "db:generate": "bunx drizzle-kit generate",
    "db:push": "bunx drizzle-kit push",
    "db:migrate": "bunx drizzle-kit migrate",
    "db:studio": "bunx drizzle-kit studio",
    "migrate": "bun run db:push",
    "generate": "bun run db:generate",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write \"src/**/*.{ts,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,json}\"",
    "typecheck": "tsc --noEmit",
    "prepare": "husky"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/bun": "latest",
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "drizzle-kit": "^0.31.8",
    "eslint": "^9.39.2",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1"
  },
  "peerDependencies": {
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.948.0",
    "@aws-sdk/s3-request-presigner": "^3.948.0",
    "@elysiajs/cookie": "^0.8.0",
    "@elysiajs/cors": "^1.4.0",
    "drizzle-orm": "^0.45.0",
    "elysia": "^1.4.18",
    "ioredis": "^5.8.2",
    "pg": "^8.16.3",
    "pino": "^10.1.0",
    "pino-elasticsearch": "^8.1.0",
    "pino-pretty": "^13.1.3",
    "pino-roll": "^4.0.0",
    "postgres": "^3.4.7",
    "prom-client": "^15.1.3",
    "uuid": "^13.0.0",
    "zod": "^4.1.13"
  }
}
